@*View for main marketing page*@

@{
    ViewData["Title"] = "Home";
}

@model ProjectListViewModel



@*Function builds each row from SQL database*@
@foreach (var x in Model.Books)
    {
        <partial name="BookList" model="x" />
    }

<br />

<div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true" 
     page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" page-url-genre="@Model.CurrentCategory">

</div>
